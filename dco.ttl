@prefix dco:     <http://info.deepcarbon.net/schema#> .
@prefix dcat:    <http://www.w3.org/ns/dcat#> .
@prefix event:   <http://purl.org/NET/c4dm/event.owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc:      <http://purl.org/dc/terms/> .
@prefix vivo:    <http://vivoweb.org/ontology/core#> .
@prefix prov:    <http://www.w3.org/ns/prov#> .
@prefix obo:     <http://purl.obolibrary.org/obo/> .
@prefix bibo:    <http://purl.org/ontology/bibo/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .

##### Ontology
<http://info.deepcarbon.net/schema>
      a       owl:Ontology ;
      rdfs:label "DCO Schema"@en-US ;
      dc:title "DCO Schema"@en-US ;
      dc:creator <http://tw.rpi.edu/instances/JohnErickson> ;
      dc:contributor <http://tw.rpi.edu/instances/PatrickWest> ;
      dc:contributor <http://tw.rpi.edu/instances/StephanZednik> ;
      dc:contributor <http://tw.rpi.edu/instances/XiaogangMa> ;
      dc:contributor <http://tw.rpi.edu/instances/PeterFox> ;
      dc:publisher <http://tw.rpi.edu/instances/TetherlessWorldConstellation> ;
      dc:date "2015-11-08"^^xsd:date ;
      dc:rights "This ontology is distributed under a Creative Commons Attribution License http://creativecommons.org/licenses/by/3.0/"@en-US;
      rdfs:comment  """
      The Deep Carbon Observatory (DCO) Ontology represents concepts and relationships used within the DCO Knowledge Store, enhancing and extending the concepts and relationships defined in the many ontologies of the VIVO Application.
      """^^rdf:XMLLiteral .

##### dco:Object
dco:Object
      a       owl:Class ;
      rdfs:label "Object"@en-US ;
      rdfs:subClassOf owl:Thing ;
      owl:equivalentClass dco:Object ;
      rdfs:comment  """
      Top level object for classes in the DCO Ontology. Any dco:Object will have a DCO Id.
      """^^rdf:XMLLiteral .

dco:DCOID
      a       owl:Class ;
      rdfs:label "DCO ID"@en-US ;
      rdfs:comment  """
      Resolvable handle from handle.net for all DCO objects.
      """^^rdf:XMLLiteral .

dco:hasDcoId
      a       owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ;
      rdfs:domain dco:Object ;
      rdfs:label "DCO ID"@en-US ;
      rdfs:range dco:DCOID ;
      owl:inverseOf dco:dcoIdFor ;
      rdfs:comment  """
      Every dco:Object has a DCOID. From the object this property points to that DCO Id.
      """^^rdf:XMLLiteral .

dco:dcoIdFor
      a       owl:ObjectProperty ;
      rdfs:domain dco:DCOID ;
      rdfs:label "DCO ID for"@en-US ;
      rdfs:range dco:Object ;
      owl:inverseOf dco:hasDcoId ;
      rdfs:comment  """
      Every dco:Object has a DCOID. From the DCOID this property points back to the dco:Object.
      """^^rdf:XMLLiteral .

dco:dcoId
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain dco:Object ;
      rdfs:label "DCO ID"@en-US ;
      rdfs:range xsd:anyURI ;
      rdfs:subPropertyOf owl:topDataProperty , dco:dcoId ;
      rdfs:comment  """
      Original representation of a DCO Id was a simple URI.
      """^^rdf:XMLLiteral .

##### foaf:Person subclasses
dco:PhDStudent
      a       owl:Class ;
      rdfs:label "Ph.D. Student"@en-US ;
      rdfs:subClassOf vivo:GraduateStudent ;
      rdfs:comment  """
      A student who is pursuing their doctorate degree in a specific field of study for a specific topic.
      """^^rdf:XMLLiteral .

dco:Engineer
      a       owl:Class ;
      rdfs:label "Engineer"@en-US ;
      rdfs:subClassOf foaf:Person , foaf:Agent , owl:Thing , dco:Object ;
      owl:equivalentClass dco:Engineer ;
      rdfs:comment  """
      A role that a person has in any of the many engineering disciplines who's goal is to design and build.
      """^^rdf:XMLLiteral .

dco:Technician
      a       owl:Class ;
      rdfs:label "Technician"@en-US ;
      rdfs:subClassOf foaf:Person , foaf:Agent , owl:Thing , dco:Object ;
      owl:equivalentClass dco:Technician ;
      rdfs:comment  """
      A role that a person has in maintaining technical equipment or doing practical work in a lab
      """^^rdf:XMLLiteral .

dco:Scientist
      a       owl:Class ;
      rdfs:label "Scientist"@en-US ;
      rdfs:subClassOf foaf:Person , owl:Thing , foaf:Agent , dco:Object ;
      owl:equivalentClass dco:Scientist ;
      rdfs:comment  """
      A role that a person has who is researching or has expert knowledge of one or more of the natural or physical sciences.
      """^^rdf:XMLLiteral .

dco:Professor
      a       owl:Class ;
      rdfs:label "Professor"@en-US ;
      rdfs:subClassOf foaf:Person , vivo:FacultyMember , owl:Thing , foaf:Agent , dco:Object ;
      owl:equivalentClass dco:Professor ;
      rdfs:comment  """
      A role that a person has as a teacher at the highest rank in a college or university.
      """^^rdf:XMLLiteral .

dco:Staff
      a       owl:Class ;
      rdfs:label "Staff"@en-US ;
      rdfs:subClassOf foaf:Person , foaf:Agent , owl:Thing , dco:Object ;
      owl:equivalentClass dco:Staff ;
      rdfs:comment  """
      A role that a person has who is assisting members of a group in carrying out assigned tasks.
      """^^rdf:XMLLiteral .

dco:skype
      a       owl:DatatypeProperty ;
      rdfs:domain foaf:Person ;
      rdfs:label "skype"@en-US ;
      rdfs:subPropertyOf owl:topDataProperty , dco:skype ;
      rdfs:comment  """
      A person's identifier used for the Skype application.
      """^^rdf:XMLLiteral .

dco:inOrganization
      a       owl:ObjectProperty ;
      rdfs:domain foaf:Person ;
      rdfs:label "organization"@en-US ;
      rdfs:range foaf:Organization ;
      rdfs:subPropertyOf dco:inOrganization , owl:topObjectProperty ;
      owl:inverseOf dco:hasPeople ;
      rdfs:comment  """
      Property which links a person with the organization that they play a role in.
      """^^rdf:XMLLiteral .

dco:shortName
      a       owl:DatatypeProperty ;
      rdfs:domain foaf:Person ;
      rdfs:label "short name"@en-US ;
      rdfs:subPropertyOf owl:topDataProperty , dco:shortName ;
      rdfs:comment  """
      The name that people commonly refer to another person has. For example, a short name of Bill for their full name William.
      """^^rdf:XMLLiteral .

dco:speaksLanguage
      a       owl:DatatypeProperty ;
      rdfs:domain foaf:Person ;
      rdfs:label "Language(s) Spoken Fluently"@en-US ;
      rdfs:comment  """
      A language that a person speaks fluently.
      """^^rdf:XMLLiteral .

dco:receivedGrant
      a       owl:ObjectProperty ;
      rdfs:domain foaf:Person ;
      rdfs:label "received grant"@en-US ;
      rdfs:range vivo:Grant ;
      owl:inverseOf dco:grantRecipient ;
      rdfs:comment  """
      Relating a person with a grant that they have received for funded work.
      """^^rdf:XMLLiteral .

dco:grantRecipient
      a       owl:ObjectProperty ;
      rdfs:domain vivo:Grant ;
      rdfs:label "recipient"@en-US ;
      rdfs:range foaf:Person ;
      owl:inverseOf dco:receivedGrant ;
      rdfs:comment  """
      Relates a grant with the person who received the grant to conduct funded work.
      """^^rdf:XMLLiteral .

dco:homeCountry
      a       owl:ObjectProperty ;
      rdfs:domain foaf:Person ;
      rdfs:label "Nationality"@en-US ;
      rdfs:range vivo:Country ;
      rdfs:subPropertyOf owl:topObjectProperty , dco:homeCountry ;
      rdfs:comment  """
      The country that a person currently resides in.
      """^^rdf:XMLLiteral .

dco:networkId
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain foaf:Person ;
      rdfs:label "network ID"@en-US ;
      rdfs:comment  """
      The property that relates a person to their DCO login.
      """^^rdf:XMLLiteral .

##### bibo:Document
dco:Artwork
      a       owl:Class ;
      rdfs:label "Artwork"@en-US ;
      rdfs:subClassOf bibo:Document , owl:Thing , dco:Object , bibo:Image , vivo:InformationResource ;
      owl:equivalentClass dco:Artwork ;
      rdfs:comment  """
      Illustrations, photographs, or other nontextual material prepared for inclusion in a publication.
      """^^rdf:XMLLiteral .

dco:Poster
      a       owl:Class ;
      rdfs:label "Poster"@en-US ;
      rdfs:subClassOf bibo:Document , owl:Thing , dco:Object , vivo:InformationResource ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty dco:isContributionToDCO
              ] ;
      owl:equivalentClass dco:Poster ;
      rdfs:comment  """
      A large printed depiction representing research or work that is being conducted.
      """^^rdf:XMLLiteral .

dco:GovernmentReport
      a       owl:Class ;
      rdfs:label "Government Report"@en-US ;
      rdfs:subClassOf bibo:Document , owl:Thing , dco:Object , vivo:InformationResource , bibo:Report ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty dco:isContributionToDCO
              ] ;
      owl:equivalentClass dco:GovernmentReport ;
      rdfs:comment  """
      An official document that is prepared to provide an account of a particular matter for a government entity.
      """^^rdf:XMLLiteral .

dco:WebArticle
      a       owl:Class ;
      rdfs:label "Web Article"@en-US ;
      rdfs:subClassOf bibo:Document , owl:Thing , dco:Object , vivo:InformationResource , bibo:Article ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty dco:isContributionToDCO
              ] ;
      owl:equivalentClass dco:WebArticle ;
      rdfs:comment  """
      A piece of writing included with others on a website.
      """^^rdf:XMLLiteral .

dco:Broadcast
      a       owl:Class ;
      rdfs:label "Broadcast"@en-US ;
      rdfs:subClassOf bibo:Document , owl:Thing , dco:Object , bibo:AudioDocument , vivo:InformationResource ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty dco:isContributionToDCO
              ] ;
      owl:equivalentClass dco:Broadcast ;
      rdfs:comment  """
      A transmission of content over a medium such as radio, television or streamed over the Internet.
      """^^rdf:XMLLiteral .

dco:Chart
      a       owl:Class ;
      rdfs:label "Chart"@en-US ;
      rdfs:subClassOf bibo:Document , owl:Thing , dco:Object , bibo:Image , vivo:InformationResource ;
      owl:equivalentClass dco:Chart ;
      rdfs:comment  """
      A graphical representation of data.
      """^^rdf:XMLLiteral .

dco:MagazineArticle
      a       owl:Class ;
      rdfs:label "Magazine Article"@en-US ;
      rdfs:subClassOf bibo:Document , owl:Thing , dco:Object , vivo:InformationResource , bibo:Article ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty dco:isContributionToDCO
              ] ;
      owl:equivalentClass dco:MagazineArticle ;
      rdfs:comment  """
      A piece of writing included with others in a magazine.
      """^^rdf:XMLLiteral .

dco:Proceedings
      a       owl:Class ;
      rdfs:label "Proceedings"@en-US ;
      rdfs:subClassOf bibo:Collection ;
      rdfs:comment  """
      A collection of academic papers published in the context of an academic conference
      """^^rdf:XMLLiteral .

dco:JournalArticle
      a       owl:Class ;
      rdfs:label "Journal Article"@en-US ;
      rdfs:subClassOf bibo:Document , bibo:AcademicArticle , owl:Thing , dco:Object , vivo:InformationResource , bibo:Article ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty dco:isContributionToDCO
              ] ;
      owl:equivalentClass dco:JournalArticle ;
      rdfs:comment  """
      A publication written by someone with academic qualifications or expert knowledge which is evidence-based and published in a
      journal.
      """^^rdf:XMLLiteral .

dco:NewspaperArticle
      a       owl:Class ;
      rdfs:label "Newspaper Article"@en-US ;
      rdfs:subClassOf bibo:Document , owl:Thing , dco:Object , vivo:InformationResource , bibo:Article ;
      owl:equivalentClass dco:NewspaperArticle ;
      rdfs:comment  """
      A piece of writing included with others in a newspaper.
      """^^rdf:XMLLiteral .

dco:BookSeries
      a       owl:Class ;
      rdfs:label "Book Series"@en-US ;
      rdfs:subClassOf bibo:Collection , owl:Thing , dco:Object , bibo:Series , vivo:InformationResource ;
      owl:equivalentClass dco:BookSeries ;
      rdfs:comment  """
      A sequence of books having certain characteristics in common that are formally identified together as a group.
      """^^rdf:XMLLiteral .

dco:isContributionToDCO
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:label "is DCO publication"@en-US ;
      rdfs:range xsd:string ;
      rdfs:comment  """
      A property which classifies a publication as being a contribution to the Deep Carbon Observatory Community.
      """^^rdf:XMLLiteral .

dco:yearOfPublication
      a       owl:DatatypeProperty ;
      rdfs:domain bibo:Document ;
      rdfs:label "year of publication"@en-US ;
      rdfs:range xsd:gYear ;
      rdfs:subPropertyOf owl:topDataProperty , dco:yearOfPublication ;
      rdfs:comment  """
      The year in which a publication has been published.
      """^^rdf:XMLLiteral .

dco:containsBook
      a       owl:ObjectProperty ;
      rdfs:domain dco:BookSeries ;
      rdfs:label "books"@en-US ;
      rdfs:range bibo:Book ;
      rdfs:subPropertyOf dco:containsBook , owl:topObjectProperty ;
      rdfs:comment  """
      One of many books that are included in a book series.
      """^^rdf:XMLLiteral .

dco:containsChapter
      a       owl:ObjectProperty ;
      rdfs:domain bibo:Book ;
      rdfs:label "chapters"@en-US ;
      rdfs:range bibo:Chapter ;
      rdfs:subPropertyOf dco:containsChapter , owl:topObjectProperty ;
      rdfs:comment  """
      One of one or more divisions of a book.
      """^^rdf:XMLLiteral .

dco:publishedInSeries
      a       owl:ObjectProperty ;
      rdfs:label "series title"@en-US ;
      rdfs:range dco:BookSeries ;
      rdfs:subPropertyOf owl:topObjectProperty , dco:publishedInSeries ;
      rdfs:comment  """
      A book or chapter of a book that is published in a series of books.
      """^^rdf:XMLLiteral .

dco:datePublished
      a       owl:ObjectProperty ;
      rdfs:domain vivo:InformationResource ;
      rdfs:label "date published"@en-US ;
      rdfs:range vivo:DateTimeValue ;
      rdfs:subPropertyOf owl:topObjectProperty , dco:datePublished , vivo:dateTimeValue ;
      rdfs:comment  """
      The date that a publication was published. Can be represented as a full date or one or more components of a date.
      """^^rdf:XMLLiteral .

dco:publishedInBook
      a       owl:ObjectProperty ;
      rdfs:label "book title"@en-US ;
      rdfs:range bibo:Book ;
      rdfs:subPropertyOf owl:topObjectProperty , dco:publishedInBook ;
      rdfs:comment  """
      A chapter, chart, figure, table of data, etc... that is included in a book.
      """^^rdf:XMLLiteral .

##### Project Updates
dco:ProjectUpdate
      a       owl:Class ;
      rdfs:label "Project Update"@en-US ;
      rdfs:subClassOf dco:Object ;
      rdfs:comment  """
      Information provided to show what progress has been made in a project during a reporting year.
      """^^rdf:XMLLiteral .

dco:hasProjectUpdate
      a       owl:ObjectProperty ;
      rdfs:domain vivo:Project ;
      rdfs:label "Project Update"@en-US ;
      rdfs:range dco:ProjectUpdate ;
      owl:inverseOf dco:forProject ;
      rdfs:comment  """
      Property relating a project to information showing progress on the project during a reporting year. There can be multiple
      project updates during a reporting year.
      """^^rdf:XMLLiteral .

dco:forProject
      a       owl:ObjectProperty ;
      rdfs:domain dco:ProjectUpdate ;
      rdfs:label "For Project"@en-US ;
      rdfs:range vivo:Project ;
      owl:inverseOf dco:hasProjectUpdate ;
      rdfs:comment  """
      Property relating a information showing progress on a project during a reporting year to the project being updated.
      """^^rdf:XMLLiteral .

dco:submittedOn
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain dco:ProjectUpdate ;
      rdfs:label "Date Submitted"@en-US ;
      rdfs:range xsd:date ;
      rdfs:comment  """
      The date on which a project update is submitted during the reporting year. Project updates are sorted on this property in
      reporting year reports.
      """^^rdf:XMLLiteral .

dco:updateText
      a       owl:DatatypeProperty ;
      rdfs:domain dco:ProjectUpdate ;
      rdfs:label "Update Text"@en-US ;
      rdfs:range xsd:string ;
      rdfs:comment  """
      The text of a project update describing the progress made on a project during a reporting year.
      """^^rdf:XMLLiteral .

dco:forReportingYear
      a       owl:FunctionalProperty , owl:ObjectProperty ;
      rdfs:domain dco:ProjectUpdate ;
      rdfs:label "For Reporting Year"@en-US ;
      rdfs:range dco:ReportingYear ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:comment  """
      Associating a reporting year to the project update.
      """^^rdf:XMLLiteral .

dco:ReportingYear
      a       owl:Class ;
      rdfs:label "Reporting Year"@en-US ;
      rdfs:subClassOf dco:Object ;
      rdfs:comment  """
      A fixed span of time defined by a funding agency after which reports are due showing progress made on projects during the
      reporting year.
      """^^rdf:XMLLiteral .

dco:startsOn
      a       owl:DatatypeProperty ;
      rdfs:domain dco:ReportingYear ;
      rdfs:label "Starts On"@en-US ;
      rdfs:range xsd:date ;
      rdfs:comment  """
      The reporting year starts on this specific date.
      """^^rdf:XMLLiteral .

dco:endsOn
      a       owl:DatatypeProperty ;
      rdfs:domain dco:ReportingYear ;
      rdfs:label "Ends On"@en-US ;
      rdfs:range xsd:date ;
      rdfs:comment  """
      The reporting year ends on this specific date.
      """^^rdf:XMLLiteral .

dco:hasReportingYear
      a       owl:ObjectProperty ;
      rdfs:domain foaf:Organization ;
      rdfs:label "Reporting Year"@en-US ;
      rdfs:range dco:ReportingYear ;
      owl:inverseOf dco:forOrganization ;
      rdfs:comment  """
      A reporting year that is defined by an organization, usually a funding agency.
      """^^rdf:XMLLiteral .

dco:forOrganization
      a       owl:ObjectProperty ;
      rdfs:domain dco:ReportingYear ;
      rdfs:label "For Organization"@en-US ;
      rdfs:range foaf:Organization ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      owl:inverseOf dco:hasReportingYear ;
      rdfs:comment  """
      The organization, usually a funding agency, that a reporting year is related to.
      """^^rdf:XMLLiteral .

dco:ProjectUpdateModificationNote
      a       owl:Class ;
      rdfs:label "Project Update Modification Note"@en-US ;
      rdfs:subClassOf dco:Object ;
      rdfs:comment  """
      Information showing who has provided information and when the information was provided for a project update.
      """^^rdf:XMLLiteral .

dco:modifiedOn
      a       owl:DatatypeProperty ;
      rdfs:domain dco:ProjectUpdateModificationNote ;
      rdfs:label "Modified On"@en-US ;
      rdfs:range xsd:date ;
      rdfs:comment  """
      The date on which an update was made to a project update.
      """^^rdf:XMLLiteral .

dco:modifiedBy
      a       owl:ObjectProperty ;
      rdfs:domain dco:ProjectUpdateModificationNote ;
      rdfs:label "Modified By"@en-US ;
      rdfs:range foaf:Person ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:comment  """
      The person who has made an update to a project update.
      """^^rdf:XMLLiteral .

dco:modificationNote
      a       owl:ObjectProperty ;
      rdfs:domain dco:ProjectUpdate ;
      rdfs:label "List of Modifications"@en-US ;
      rdfs:range dco:ProjectUpdateModificationNote ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      owl:inverseOf dco:forProjectUpdate ;
      rdfs:comment  """
      A description of what modifications were made to a project update, such as the attachment of a publication or clarifying
      information in the update.
      """^^rdf:XMLLiteral .

dco:forProjectUpdate
      a       owl:ObjectProperty ;
      rdfs:domain dco:ProjectUpdateModificationNote ;
      rdfs:label "For Project Update"@en-US ;
      rdfs:range dco:ProjectUpdate ;
      owl:inverseOf dco:modificationNote ;
      rdfs:comment  """
      The project update to which a modification information is added.
      """^^rdf:XMLLiteral .

dco:referencedByUpdate
      a       owl:ObjectProperty ;
      rdfs:domain obo:ERO_0000004 ;
      rdfs:label "Referenced By Update"@en-US ;
      rdfs:range dco:ProjectUpdate ;
      owl:inverseOf dco:refersToInstrument ;
      rdfs:comment  """
      Something that is referenced from within project update information during a specific reporting year.
      """^^rdf:XMLLiteral .

dco:associatedPublication
      a       owl:ObjectProperty ;
      rdfs:domain dco:ProjectUpdate ;
      rdfs:label "Associated Publications"@en-US ;
      rdfs:range bibo:Document ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:comment  """
      A publication referenced by a project update during a specific reporting year.
      """^^rdf:XMLLiteral .

dco:refersToInstrument
      a       owl:ObjectProperty ;
      rdfs:domain dco:ProjectUpdate ;
      rdfs:label "Refers to Instrument"@en-US ;
      rdfs:range obo:ERO_0000004 ;
      owl:inverseOf dco:referencedByUpdate ;
      rdfs:comment  """
      An instrument, such as a satellite, that is used by a project and referenced in a project update.
      """^^rdf:XMLLiteral .

##### Field Studies
dco:FieldStudy
      a       owl:Class ;
      rdfs:label "Field Study"@en-US ;
      rdfs:subClassOf owl:Thing , dco:Activity , vivo:Project , dco:Object ;
      owl:equivalentClass dco:FieldStudy ;
      rdfs:comment  """
      A type of project where researchers visit a particular site in order to collect measurements, samples and other data.
      """^^rdf:XMLLiteral .

dco:hasPhysicalLocation
      a       owl:ObjectProperty ;
      rdfs:domain dco:FieldStudy ;
      rdfs:label "Field sites"@en-US ;
      rdfs:range dco:PhysicalLocation ;
      owl:inverseOf dco:isPhysicalLocationOf ;
      rdfs:comment  """
      Relating a field study with a specific location where measurements, samples and other data are collected.
      """^^rdf:XMLLiteral .

dco:PhysicalLocation
      a       owl:Class ;
      rdfs:label "Physical Location"@en-US ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty vivo:description
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty dco:hasLongitude
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty dco:hasLatitude
              ] ;
      rdfs:comment  """
      The location where measurements, samples and other data are collected. Must include a description, longitude and latitude.
      """^^rdf:XMLLiteral .

dco:isPhysicalLocationOf
      a       owl:ObjectProperty ;
      rdfs:domain dco:PhysicalLocation ;
      rdfs:label "is field site of"@en-US ;
      rdfs:range dco:FieldStudy ;
      owl:inverseOf dco:hasPhysicalLocation ;
      rdfs:comment  """
      Relating a representation of a physical location where measurements are taken to a field study.
      """^^rdf:XMLLiteral .

dco:pressure
      a       owl:DatatypeProperty ;
      rdfs:domain dco:PhysicalLocation ;
      rdfs:label "pressure"@en-US ;
      rdfs:range xsd:string ;
      rdfs:comment  """
      Measurement of force per unit area that a fluid exerts on its surroundings.
      """^^rdf:XMLLiteral .

dco:depth
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain dco:PhysicalLocation ;
      rdfs:label "depth (meters)"@en-US ;
