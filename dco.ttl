      """^^rdf:XMLLiteral .

dco:ResearchAdministration
      a       owl:Class ;
      rdfs:label "Research Administration"@en-US ;
      rdfs:subClassOf vivo:ResearchOrganization , foaf:Organization , owl:Thing , foaf:Agent , dco:Object ;
      owl:equivalentClass dco:ResearchAdministration ;
      rdfs:comment  """
      An group of people who administer the resources of a research
      community.
      """^^rdf:XMLLiteral .

dco:ResearchAdvisoryCommittee
      a       owl:Class ;
      rdfs:label "Research Advisory Committee"@en-US ;
      rdfs:subClassOf vivo:ResearchOrganization , foaf:Organization , foaf:Agent , owl:Thing , dco:Object ;
      owl:equivalentClass dco:ResearchAdvisoryCommittee ;
      rdfs:comment  """
      a collection of individuals who bring unique knowledge and skills
      which augment the knowledge and skills of the research community
      """^^rdf:XMLLiteral .

dco:associatedDCOCommunity
      a       owl:ObjectProperty ;
      rdfs:label "associated DCO community"@en-US ;
      rdfs:range dco:ResearchCommunity ;
      rdfs:comment  """
      Relates a research community to an entity such as a publication or
      dataset.
      """^^rdf:XMLLiteral .

dco:associatedDCOPortalGroup
      a       owl:ObjectProperty ;
      rdfs:label "associated DCO portal group"@en-US ;
      rdfs:range dco:PortalGroup ;
      rdfs:subPropertyOf dco:associatedDCOPortalGroup , owl:topObjectProperty ;
      rdfs:comment  """
      Relates a portal group to an entity such as a publication or
      dataset.
      """^^rdf:XMLLiteral .

dco:hasPeople
      a       owl:ObjectProperty ;
      rdfs:domain foaf:Organization ;
      rdfs:label "has people"@en-US ;
      rdfs:range foaf:Person ;
      rdfs:subPropertyOf owl:topObjectProperty , dco:hasPeople ;
      owl:inverseOf dco:inOrganization ;
      rdfs:comment  """
      An individual who is a part of an organization.
      """^^rdf:XMLLiteral .

dco:relatedOrganization
      a       owl:ObjectProperty , owl:SymmetricProperty ;
      rdfs:domain foaf:Organization ;
      rdfs:label "related organization"@en-US ;
      rdfs:range foaf:Organization ;
      rdfs:subPropertyOf dco:relatedOrganization , owl:topObjectProperty ;
      owl:inverseOf dco:relatedOrganization ;
      rdfs:comment  """
      Relates one organization to another organization.
      """^^rdf:XMLLiteral .

##### Distributions, Files, Datasets
dco:File
      a       owl:Class ;
      rdfs:label "File"@en-US ;
      rdfs:subClassOf owl:Thing , dco:Object ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom dco:ResearchCommunity ;
                owl:onProperty dco:associatedDCOCommunity
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom rdfs:Literal ;
                owl:onProperty dco:downloadURL
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom dco:PortalGroup ;
                owl:onProperty dco:associatedDCOPortalGroup
              ] ;
      owl:equivalentClass dco:File ;
      rdfs:comment  """
      A self contained collection of data or information available in a
      particular format and available to computer programs.
      """^^rdf:XMLLiteral .

dco:hasDistribution
      a       owl:ObjectProperty ;
      rdfs:domain vivo:InformationResource ;
      rdfs:label "distribution (attachments or links)"@en-US ;
      rdfs:range dcat:Distribution ;
      rdfs:subPropertyOf dco:hasDistribution , owl:topObjectProperty ;
      owl:inverseOf dco:distributionFor ;
      rdfs:comment  """
      Links a collection of data or information to a particular
      available form of that information. Each dataset might be
      available in different forms, these forms might represent
      different formats of the dataset or different endpoints.
      Examples of distributions include a downloadable CSV file,
      an API or an RSS feed.
      """^^rdf:XMLLiteral .

dco:hasFile
      a       owl:ObjectProperty ;
      rdfs:domain dcat:Distribution ;
      rdfs:label "files or links to files"@en-US ;
      rdfs:range dco:File ;
      rdfs:subPropertyOf owl:topObjectProperty , dco:hasFile ;
      owl:inverseOf dco:fileFor ;
      rdfs:comment  """
      Links a distribution of a datasets to a specific file that is a
      part of that distribution.
      """^^rdf:XMLLiteral .

dco:isDatasetOf
      a       owl:ObjectProperty ;
      rdfs:domain vivo:Dataset ;
      rdfs:label "Is Dataset Of"@en-US ;
      rdfs:range vivo:Project ;
      owl:inverseOf dco:relatedDataset ;
      rdfs:comment  """
      Links a dataset back to the project from which the data was
      collected.
      """^^rdf:XMLLiteral .

dco:distributionFor
      a       owl:ObjectProperty ;
      rdfs:domain dcat:Distribution ;
      rdfs:label "Distribution (attachment or links) for"@en-US ;
      rdfs:range vivo:InformationResource ;
      rdfs:subPropertyOf owl:topObjectProperty , dco:distributionFor ;
      owl:inverseOf dco:hasDistribution ;
      rdfs:comment  """
      Links a distribution back to the information resource, such as
      publication or dataset.
      """^^rdf:XMLLiteral .

dco:fileFor
      a       owl:ObjectProperty ;
      rdfs:domain dco:File ;
      rdfs:label "file or link for"@en-US ;
      rdfs:range dcat:Distribution ;
      rdfs:subPropertyOf owl:topObjectProperty , dco:fileFor ;
      owl:inverseOf dco:hasFile ;
      rdfs:comment  """
      Links a container of data or information back to the
      distribution that it is a part of.
      """^^rdf:XMLLiteral .

dco:hasDataset
      a       owl:ObjectProperty ;
      rdfs:domain foaf:Person ;
      rdfs:label "Add Dataset"@en-US ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:comment  """
      Links a person to a dataset that they participated in the creation
      of.
      """^^rdf:XMLLiteral .

dco:hasDataType
      a       owl:ObjectProperty ;
      rdfs:comment "A dataset has one or more data types." ;
      rdfs:domain vivo:Dataset ;
      rdfs:label "has data type"@en-US ;
      rdfs:range dco:DataType ;
      rdfs:comment  """
      Links a collection of data to a particular kind of data item that
      is contained within it.
      """^^rdf:XMLLiteral .

dco:relatedDataset
      a       owl:FunctionalProperty , owl:ObjectProperty ;
      rdfs:domain vivo:Project ;
      rdfs:label "Related datasets"@en-US ;
      rdfs:range vivo:Dataset ;
      owl:inverseOf dco:isDatasetOf ;
      rdfs:comment  """
      Relates a dataset to a project that created or used it.
      """^^rdf:XMLLiteral .

