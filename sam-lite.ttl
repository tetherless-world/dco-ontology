  rdfs:comment "Simple model for elevation, expressed as a vertical offset from a datum"^^xsd:string ;
  rdfs:isDefinedBy <http://def.seegrid.csiro.au/ontology/om/sam-lite> ;
  rdfs:label "Elevation"^^xsd:string ;
