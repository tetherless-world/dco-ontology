samfl:SamplingSurface
  rdf:type owl:Class ;
  dct:description "Spatial sampling feature whose shape is a 2-D manifold (Surface)"^^xsd:string ;
  rdfs:comment """Spatial sampling feature whose shape is a 2-D manifold (Surface)

